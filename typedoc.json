{
	"out": "./api",
	"json": "./api/docs.json",
	"name": "Color.js API Docs",
	"entryPoints": [
		"types/src/color.d.ts",
		"types/src/space.d.ts",
		"types/src/index-fn.d.ts",
		"types/src/spaces/index.d.ts",
		"types/src/deltaE/index.d.ts"
	],
	"entryPointStrategy": "expand",
	"plugin": [
		"typedoc-plugin-rename-defaults"
	],
	"excludeExternals": true,
	"includeVersion": true,
	"kindSortOrder": [
		"Project",
		"Module",
		"Enum",
		"EnumMember",
		"Class",
		"Namespace",
		"Interface",
		"TypeAlias",
		"Constructor",
		"Property",
		"Method",
		"Function",
		"Accessor",
		"Variable",
		"Reference",
		"Parameter",
		"TypeParameter",
		"TypeLiteral",
		"CallSignature",
		"ConstructorSignature",
		"IndexSignature",
		"GetSignature",
		"SetSignature"
	]
}
